<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <bean id="org.dozer.Mapper" class="org.dozer.DozerBeanMapper">
        <property name="mappingFiles">
            <list>
                <value>mapping/view-mapper.xml</value>
            </list>
        </property>
    </bean>


    <bean id="findUnitsInteractor" class="com.agaveazul.interactor.FindUnitsInteractorImpl" >
        <property name="unitGateway" ref="unitGateway"/>
    </bean>

    <bean id="saveUnitInteractor" class="com.agaveazul.interactor.SaveUnitInteractorImpl" >
        <property name="unitGateway" ref="unitGateway"/>
    </bean>
</beans>